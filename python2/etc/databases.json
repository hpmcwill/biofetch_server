{
  "embl":{
    "label": "EMBL-Bank",
    "description": "The european nucleotide sequence archive database.",
    "databaseUrl": "http://www.ebi.ac.uk/ena/",
    "defaultDataFormat": "embl",
    "defaultResultStyles":{
      "embl": "html",
      "fasta": "html",
      "annot": "raw"
    },
    "serviceList":[
      {
        "serviceName": "SRS@DKFZ",
        "serviceUrl": "http://www.dkfz.de/srs/",
        "primaryAccess":{
          "accessType": "url",
          "urlTemplate": "http://www.dkfz.de/menu/cgi-bin/srs7.1.3.1/wgetz?[${db}-acc:${id}]${format}${style}+-noSession",
          "multipleIdentifierSupport": true,
          "idSeparator": "|"
        },
        "dataFormats":{
          "embl":{
            "value": "+-e+-vn+2",
            "resultStyles":{
              "html": "",
              "raw": "+-ascii"
            }
          },
          "fasta":{
            "value": "+-f+seq+-sf+fasta",
            "resultStyles":{
              "html": "",
              "raw": "+-ascii"
            }
          }
        }
      },
      {
        "serviceName": "ENA Browser",
        "serviceUrl": "http://www.ebi.ac.uk/ena/",
        "primaryAccess":{
          "accessType": "url",
          "urlTemplate": "http://www.ebi.ac.uk/ena/data/view/${id}${format}&limit=${maxEntries}",
          "multipleIdentifierSupport": true,
          "idSeparator": ",",
          "idCharacterCase": "upper"
        },
        "dataFormats":{
          "embl":{
            "value": "&display=text",
            "resultStyles":{
              "html": "",
              "raw": ""
            }
          },
          "fasta":{
            "value": "&display=fasta",
            "resultStyles":{
              "html": "",
              "raw": ""
            }
          }
        }
      }
    ]
  }
}